        <div class='dashboard'>
            <div class="dashboard-nav">
                <header>
                    <a class="brand-logo"><i class="bi bi-person-circle"></i>
                        <span>Usuario</span>
                    </a>
                </header>
                <nav class="dashboard-nav-list">
                    <div class="nav-item-divider"></div>
                    <a class="dashboard-nav-item" asp-area="" asp-controller="Home" asp-action="Index"> Home</a>
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy"><i class="bi bi-incognito" style="color: goldenrod;"></i>  Privacy</a>
                    <a class="nav-link text-dark" asp-area="" asp-controller="Usuario" asp-action="GetAll"><i class="bi bi-person-lines-fill" style="color: goldenrod;"></i> Usuarios</a>
                    <a class="nav-link text-dark" asp-area="" asp-controller="Departamento" asp-action="GetAll"><i class="bi bi-houses" style="color: goldenrod;"></i> Departamentos</a>
                    <a class="nav-link text-dark" asp-area="" asp-controller="Producto" asp-action="GetAll"><i class="bi bi-journal-bookmark-fill" style="color: goldenrod;"></i> Productos</a>
                    <a class="nav-link text-dark" asp-area="" asp-controller="VentaProducto" asp-action="GetAll"><i class="bi bi-currency-dollar" style="color: goldenrod;"></i> Venta de productos</a>
                    <a class="nav-link text-dark" asp-area="" asp-controller="VentaProducto" asp-action="ResumenCompra"><i class="bi bi-cart-fill" style="color: goldenrod;"></i> Compras</a>
                    <a class="nav-link text-dark" asp-area="" asp-controller="CargaMasiva" asp-action="UsuarioCargaMasiva"><i class="bi bi-capslock-fill" style="color: goldenrod;"></i> Carga masiva</a>

                    <div class="nav-item-divider"></div>
                    <a href="#" class="dashboard-nav-item"><i class="fas fa-sign-out-alt"></i> Logout </a>
                </nav>
            </div>
            <div class='dashboard-app'>
                <div class='dashboard-content'>
                    <div class='container'>
                        <main role="main" class="pb-3">
                            @RenderBody()
                        </main>
                    </div>
                </div>
            </div>
        </div>
   

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - PL - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
